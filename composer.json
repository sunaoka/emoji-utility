{
    "name": "sunaoka/emoji-utility",
    "description": "Utilities for Emoji using PHP",
    "keywords": ["emoji", "utility" ,"parser", "emoji-test.txt"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "symfony/polyfill-php80": "^1.28"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\EmojiUtility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\EmojiUtility\\Tests\\": "tests/"
        }
    }
}
